{
    "name": "artisanry/geoip2",
    "description": "A Laravel 5 package for MaxMind GeoIP2 support.",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-GeoIP2",
        "Laravel GeoIP2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "geoip2/geoip2": "~2.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\GeoIp2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\GeoIp2\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\GeoIp2\\GeoIp2ServiceProvider"
            ],
            "aliases": {
                "GeoIp2": "Artisanry\\GeoIp2\\Facades\\GeoIp2"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
